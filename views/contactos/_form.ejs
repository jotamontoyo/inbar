
<div class="form-group" style="margin-top: 120px">
	<label for="preg" class="col-sm-2 control-label">Nombre:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="nombre" name="contacto[nombre]" value="<%= contacto.nombre %>" placeholder="nombre"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Alias:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="alias" name="contacto[alias]" value="<%= contacto.alias %>" placeholder="alias"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">eMail:</label>
	<div class="col-sm-10">
		<input type="email" class="form-control" id="email" name="contacto[email]" value="<%= contacto.email %>" placeholder="eMail"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">eMail:</label>
	<div class="col-sm-10">
		<input type="email" class="form-control" id="emailok" name="contacto[emailok]" value="<%= contacto.emailok %>" placeholder="eMail confirm"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Teléfono:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="telf" name="contacto[telefono]" value="<%= contacto.telefono %>" placeholder="teléfono"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Marca:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="marca" name="contacto[marca]" value="<%= contacto.marca %>" placeholder="marca"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Ciudad:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="ciudad" name="contacto[ciudad]" value="<%= contacto.ciudad %>" placeholder="ciudad"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Provincia:</label>
	<div class="col-sm-10">
		<input type="text" class="form-control" id="provincia" name="contacto[provincia]" value="<%= contacto.provincia %>" placeholder="provincia"/>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Revisado:</label>
	<div class="col-sm-10">
		<select class="form-control" id="revisado" name="contacto[revisado]">
			<option value=true <%if (contacto.revisado){%> selected <%}%>>Revisado</option>
			<option value=false <%if (!contacto.revisado){%> selected <%}%>>Pendiente</option>
		</select>
	</div>
</div>

<div class="form-group">
	<label for="preg" class="col-sm-2 control-label">Comentarios:</label>
	<textarea class="form-control" id="comentario" name="contacto[comentario]" rows="6" value="<%= contacto.comentario %>" style="width: 109%; max-width: 888px" placeholder="aquí tus comentarios"><%= contacto.comentario %></textarea>
</div>

<div class="form-group">
	<div class="col-sm-offset-2 col-sm-10">
		<a href="/contactos">
			<button class="btn btn-danger btn-sm">
				<span class="glyphicon glyphicon-remove" aria-hidden="true"></span> Cancelar
			</button>
		</a>
		<button class="btn btn-success btn-sm" type="submit" value="Salvar">
			<span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Grabar
		</button>
	</div>
</div>
